# Runtime Mode (mock | real)
# Default: mock (no credentials required)
# Set to 'real' to use actual UPS API (requires credentials)
CARRIER_MODE=mock

# UPS Configuration (required only when CARRIER_MODE=real)
UPS_BASE_URL=https://wwwcie.ups.com
UPS_OAUTH_TOKEN_URL=https://wwwcie.ups.com/security/v1/oauth/token
UPS_CLIENT_ID=your_ups_client_id
UPS_CLIENT_SECRET=your_ups_client_secret
UPS_OAUTH_SCOPE=rating
UPS_SHIPPER_NUMBER=your_shipper_number

# Future carrier configurations (not yet implemented)
# FEDEX_API_KEY=your_fedex_api_key
# USPS_USER_ID=your_usps_user_id
# DHL_API_KEY=your_dhl_api_key
